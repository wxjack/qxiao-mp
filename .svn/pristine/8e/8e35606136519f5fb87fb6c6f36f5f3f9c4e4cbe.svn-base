@click=\"handleRouteGo\">\r\n        <h3 class=\"text-ellipsis\">{{ f.title }}</h3>\r\n        <div style=\"color:#8d8d8d;\">\r\n          <time>{{ f.postTime }}</time>\r\n        </div>\r\n        <img src=\"http://iph.href.lu/690x298\" alt=\"\">\r\n        <p class=\"line-clamp\">{{ f.textContent }}</p>\r\n        <div class=\"metedata\" style=\"color:#8d8d8d;\">\r\n          <span>{{ f.classReadCount }}人阅读</span>\r\n          <span>留言{{ f.classCommentCount }}</span>\r\n        </div>\r\n      </figure>  \r\n    </div>\r\n  </div>  \r\n</template>\r\n<script>\r\nimport service from \"@/api\";\r\nexport default {\r\n  name: \"fresh\",\r\n  data() {\r\n    return {\r\n      className: \"选择班级查看\",\r\n      classList: [],\r\n      query: {\r\n        openId: this.$store.getters.openId,\r\n        classId: 0\r\n      },\r\n      queryClass: {\r\n        id: this.$store.getters.id,\r\n        roleType: this.$store.getters.roleType\r\n      },\r\n      freshData: []\r\n    };\r\n  },\r\n  methods: {\r\n    handleRouteGo() {\r\n      //this.$router.push({ path: \"/fresh/show\" });\r\n    },\r\n    handleSelectClass() {\r\n      this.$weui.picker(this.classList, {\r\n        defaultValue: [this.query.classId],\r\n        onConfirm: result => {\r\n          let value = result[0].value; //取第一个元素\r\n          let label = result[0].label;\r\n        }\r\n      });\r\n    },\r\n    //根据类型查询相关班级\r\n    async queryClassId(params = {}) {\r\n      let res = await service.queryClassId(params);\r\n      if (res.errorCode === 0) {\r\n        let classMap = res.data.map(item => {\r\n          return {\r\n            label: item.className,\r\n            value: item.classId\r\n          };\r\n        });\r\n        this.classList = classMap;\r\n      }\r\n    },\r\n    //速报列表查询\r\n    async freshQuery(params = {}) {\r\n      let res = await service.freshQuery(params);\r\n      if (res.errorCode === 0) {\r\n        this.freshData = res.data;\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.queryClassId(this.queryClass);\r\n  },\r\n  activated() {\r\n    this.freshQuery(this.query);\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\">\r\n.page-hd {\r\n  margin-bottom: 20px;\r\n  background-color: #fff;\r\n}\r\n.button-sp-area {\r\n  color: #9cd248;\r\n  height: 100px;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.figure {\r\n  padding: 0 30px;\r\n  margin-bottom: 30px;\r\n  background-color: #fff;\r\n  &:active {\r\n    background-color: #f2f2f2;\r\n  }\r\n  h3 {\r\n    font-size: 36px;\r\n    padding: 20px 0;\r\n  }\r\n  p {\r\n    font-size: 28px;\r\n  }\r\n  img {\r\n    width: 690px;\r\n    height: 298px;\r\n    margin: 20px 0;\r\n  }\r\n  .metedata {\r\n    margin: 20px -30px 0 -30px;\r\n    padding: 0 30px;\r\n    display: flex;\r\n    height: 80px;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    border-top: 1px solid #f5f5f5;\r\n  }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/views/fresh/index.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"page\"},[_c('div',{staticClass:\"page-hd\"},[_c('div',{staticClass:\"button-sp-area flex\",attrs:{\"size-17\":\"\"}},[_c('a',{attrs:{\"href\":\"javascript:void(0);\"},on:{\"click\":_vm.handleSelectClass}},[_c('span',[_vm._v(_vm._s(_vm.className))])])])]),_vm._v(\" \"),_c('div',{staticClass:\"page-bd\"},[_c('router-link',{staticClass:\"release\",attrs:{\"to\":\"/fresh/add\"}},[_c('img',{attrs:{\"src\":require(\"@/assets/image/release-icon.png\"),\"alt\":\"\"}})]),_vm._v(\" \"),_vm._l((_vm.freshData),function(f,index){return _c('figure',{key:index,staticClass:\"figure\",on:{\"click\":_vm.handleRouteGo}},[_c('h3',{staticClass:\"text-ellipsis\"},[_vm._v(_vm._s(f.title))]),_vm._v(\" \"),_c('div',{staticStyle:{\"color\":\"#8d8d8d\"}},[_c('time',[_vm._v(_vm._s(f.postTime))])]),_vm._v(\" \"),_c('img',{attrs:{\"src\":\"http://iph.href.lu/690x298\",\"alt\":\"\"}}),_vm._v(\" \"),